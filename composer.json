{
    "name": "yiisoft/yii2-app-advanced",
    "description": "Yii 2 Advanced Project Template",
    "keywords": ["yii2", "framework", "advanced", "project template"],
    "homepage": "http://www.yiiframework.com/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii2/issues?state=open",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii2"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.0.0",
        "yiisoft/yii2": "~2.0.14",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "yiisoft/yii2-swiftmailer": "~2.1.0",
        "yiisoft/yii2-imagine": "~2.1.0",
        "yiisoft/yii2-httpclient": "~2.0.0",
        "yiisoft/yii2-redis": "^2.0",
        "yiisoft/yii2-jui": "^2.0",
        "yiisoft/yii2-queue": "^2.0",
        "yii2tech/authlog": "*",
        "kartik-v/yii2-widgets": "*",
        "kartik-v/yii2-grid": "@dev",
        "kartik-v/yii2-dialog": "@dev",
        "kartik-v/yii2-datecontrol": "@dev",
        "kartik-v/dependent-dropdown": "@dev",
        "kartik-v/yii2-checkbox-x": "@dev",
        "kartik-v/yii2-detail-view": "@dev",
        "kartik-v/yii2-editable": "@dev",
        "kartik-v/yii2-password": "@dev",
        "kartik-v/yii2-mpdf": "*",
        "kartik-v/yii2-widget-datetimepicker": "*",
        "kartik-v/yii2-widget-depdrop": "@dev",
        "voskobovich/yii2-linker-behavior": "^4.0",
        "himiklab/yii2-recaptcha-widget": "^1.2",
        "codemix/yii2-localeurls": "^1.5",
        "2amigos/yii2-tinymce-widget": "~1.1",
        "2amigos/yii2-usuario": "~1.0",
        "ignatenkovnikita/yii2-queuemanager": "dev-master",
        "cebe/yii2-gravatar": "^1.0",
        "asinfotrack/yii2-audittrail": "dev-master",
        "yii2tech/ar-linkmany": "*",
        "brussens/yii2-maintenance-mode": "*",
        "dmstr/yii2-adminlte-asset": "^2.1",
        "bower-asset/ekko-lightbox": "*",
        "skeeks/yii2-assets-auto-compress": "*",
        "2amigos/yii2-gallery-widget": "~1.0",
        "yii2mod/yii2-google-maps-markers": "*"
        
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.0.0",
        "yiisoft/yii2-gii": "~2.0.0",
        "yiisoft/yii2-faker": "~2.0.0",
        "codeception/codeception": "*",
        "codeception/specify": "*",
        "codeception/verify": "*",
        "yiisoft/yii2-coding-standards": "*",
        "symfony/var-dumper": "^3.3",
        "mootensai/yii2-enhanced-gii": "dev-master",
        "warrence/yii2-kartikgii": "dev-master",
        "phpunit/php-invoker": "^1.1",
        "insolita/yii2-migration-generator": "~2.3"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm-asset",
            "bower-asset-library": "vendor/bower-asset"
        }
    }
}
